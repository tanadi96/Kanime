generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Collection {
  id          Int      @id @default(autoincrement())
  mal_id      String
  user_email  String
  image_url   String?
  title       String?
  
 @@unique([mal_id, user_email])
}

model Comment{
  id          Int      @id @default(autoincrement())
  mal_id      String
  user_email  String
  comment     String
  username    String
  title       String
  created_at  DateTime @default(now())
  
}